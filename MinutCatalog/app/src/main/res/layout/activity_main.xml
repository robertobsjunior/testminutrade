<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.catalog.minut.minutecatalog.LoginActivity">



    <com.google.android.gms.common.SignInButton
        android:id="@+id/sign_in_button"
        android:layout_width="276dp"
        android:layout_height="49dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteY="232dp"
        tools:layout_editor_absoluteX="10dp" />

    <fragment
        android:id="@+id/fragmentFacebook"
        android:name="com.catalog.minut.minutecatalog.FacebookLoginFragment"
        android:layout_width="276dp"
        android:layout_height="49dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/sign_in_button"
        app:layout_constraintRight_toRightOf="@+id/sign_in_button"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/sign_in_button" />


</android.support.constraint.ConstraintLayout>
